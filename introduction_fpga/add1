    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
    .port_info 3 /OUTPUT 1 "carry";
L_0x5bdb4a6c8b40 .functor XOR 1, v0x5bdb4a6c87e0_0, v0x5bdb4a6c88a0_0, C4<0>, C4<0>;
L_0x5bdb4a6c8ca0 .functor AND 1, v0x5bdb4a6c87e0_0, v0x5bdb4a6c88a0_0, C4<1>, C4<1>;
v0x5bdb4a6b5b90_0 .net "a", 0 0, v0x5bdb4a6c87e0_0;  1 drivers
v0x5bdb4a6c8540_0 .net "b", 0 0, v0x5bdb4a6c88a0_0;  1 drivers
v0x5bdb4a6c8600_0 .net "carry", 0 0, L_0x5bdb4a6c8ca0;  alias, 1 drivers
v0x5bdb4a6c86a0_0 .net "sum", 0 0, L_0x5bdb4a6c8b40;  alias, 1 drivers
    .scope S_0x5bdb4a6b57b0;
T_0 ;
    %vpi_call 2 18 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5bdb4a6b57b0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bdb4a6c87e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bdb4a6c88a0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5bdb4a6c87e0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5bdb4a6c88a0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bdb4a6c88a0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5bdb4a6c87e0_0, 0, 1;
    %delay 40000, 0;
    %end;
    .thread T_0;
    .scope S_0x5bdb4a6b57b0;
T_1 ;
    %vpi_call 2 33 "$monitor", "t=%3d a=%d,b=%d,sum=%d,carry=%d \012", $time, v0x5bdb4a6c87e0_0, v0x5bdb4a6c88a0_0, v0x5bdb4a6c8a70_0, v0x5bdb4a6c8970_0 {0 0 0};
