S_0x5bdb4a6b57b0 .scope module, "stimulus" "stimulus" 2 2;
 .timescale -9 -12;
v0x5bdb4a6c87e0_0 .var "a", 0 0;
v0x5bdb4a6c88a0_0 .var "b", 0 0;
v0x5bdb4a6c8970_0 .net "carry", 0 0, L_0x5bdb4a6c8ca0;  1 drivers
v0x5bdb4a6c8a70_0 .net "sum", 0 0, L_0x5bdb4a6c8b40;  1 drivers
S_0x5bdb4a6b5940 .scope module, "uut" "add1" 2 10, 3 1 0, S_0x5bdb4a6b57b0;
