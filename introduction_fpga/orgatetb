#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x638555f2a240 .scope module, "stimulus" "stimulus" 2 2;
 .timescale -9 -12;
v0x638555f3c880_0 .var "x", 0 0;
v0x638555f3c950_0 .var "y", 0 0;
v0x638555f3ca20_0 .net "z", 0 0, L_0x638555f3cb20;  1 drivers
S_0x638555f2a3d0 .scope module, "uut" "or_gate" 2 9, 3 1 0, S_0x638555f2a240;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "e1";
    .port_info 1 /INPUT 1 "e2";
    .port_info 2 /OUTPUT 1 "s";
L_0x638555f3cb20 .functor OR 1, v0x638555f3c880_0, v0x638555f3c950_0, C4<0>, C4<0>;
v0x638555f2a600_0 .net "e1", 0 0, v0x638555f3c880_0;  1 drivers
v0x638555f3c6a0_0 .net "e2", 0 0, v0x638555f3c950_0;  1 drivers
v0x638555f3c760_0 .net "s", 0 0, L_0x638555f3cb20;  alias, 1 drivers
    .scope S_0x638555f2a240;
T_0 ;
    %vpi_call 2 16 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x638555f2a240 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638555f3c880_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638555f3c950_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x638555f3c880_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x638555f3c950_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638555f3c950_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x638555f3c880_0, 0, 1;
    %delay 40000, 0;
    %end;
    .thread T_0;
    .scope S_0x638555f2a240;
T_1 ;
    %vpi_call 2 31 "$monitor", "t=%3d x=%d,y=%d,z=%d \012", $time, v0x638555f3c880_0, v0x638555f3c950_0, v0x638555f3ca20_0, " " {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_or_gate.v";
    "or_gate.v";
